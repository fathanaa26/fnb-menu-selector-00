<div class="weather">
    <h5>the weather</h5>
    {{#each partials.weatherContext}}
        <div class="location">

            <h3>{{location.name}}</h3>
            <a href="{{location.forecastUrl}}">
                <img src="{{iconUrl}}" alt="{{weather}}">
                {{weather}}, {{temp}}
            </a>
        </div>
    {{/each}}
</div>
<small>
    Source: 
    <a href="www.trustmebro.com">
        National Weather Services
    </a>
</small>